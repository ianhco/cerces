{
    "name": "apertum",
    "version": "1.0.0-alpha.0",
    "description": "A modern, OpenAPI-based, and fully typed web framework.",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/src/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/src/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    },
    "files": [
        "dist",
        "LICENSE",
        "README.md"
    ],
    "scripts": {
        "build": "rollup -c",
        "package": "npm run build && npm pack",
        "prettify": "prettier --check src README.md || (prettier -w src README.md)",
        "prepare": "husky install",
        "test": "jest --no-cache --runInBand --config jestconfig.json --verbose",
        "test:cov": "jest --no-cache --runInBand --config jestconfig.json --verbose --coverage"
    },
    "publishConfig": {
        "access": "public"
    },
    "keywords": [
        "worker",
        "workers",
        "serverless",
        "cloudflare workers",
        "router",
        "openapi",
        "swagger",
        "openapi generator",
        "cf",
        "optional",
        "middleware",
        "parameters",
        "typescript",
        "npm",
        "package",
        "cjs",
        "esm",
        "umd",
        "typed"
    ],
    "author": "iann838dev@gmail.com",
    "license": "MIT",
    "homepage": "https://github.com/iann838/apertum",
    "repository": {
        "type": "git",
        "url": "git@github.com:iann838/apertum.git"
    },
    "bugs": {
        "url": "https://github.com/iann838/apertum/issues"
    },
    "devDependencies": {
        "@commitlint/cli": "^13.1.0",
        "@commitlint/config-conventional": "^13.1.0",
        "@rollup/plugin-terser": "^0.2.1",
        "@rollup/plugin-typescript": "^10.0.1",
        "@types/jest": "^29.0.0",
        "@types/node": "^20.3.1",
        "husky": "^7.0.2",
        "jest": "^29.0.0",
        "jest-openapi": "^0.14.2",
        "prettier": "^3.1.0",
        "rollup": "^3.25.1",
        "rollup-plugin-bundle-size": "^1.0.3",
        "rollup-plugin-copy": "^3.4.0",
        "ts-jest": "^29.1.0",
        "typescript": "^5.1.3"
    },
    "dependencies": {
        "@asteasolutions/zod-to-openapi": "^5.3.1",
        "@types/cookie": "^0.6.0",
        "cookie": "^0.6.0",
        "openapi3-ts": "^4.1.2",
        "zod": "^3.21.4"
    }
}
