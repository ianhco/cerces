{
    "name": "cerces",
    "version": "2.0.1-beta.5",
    "description": "Type-safe, modern, and intuitive web framework built on Web Standards and OpenAPI.",
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "engines": {
        "node": ">=18.0.0"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        },
        "./types": {
            "types": "./dist/types.d.ts"
        },
        "./core": {
            "types": "./dist/core.d.ts",
            "import": "./dist/core.js",
            "require": "./dist/core.cjs"
        },
        "./docs": {
            "types": "./dist/docs.d.ts",
            "import": "./dist/docs.js",
            "require": "./dist/docs.cjs"
        },
        "./helpers": {
            "types": "./dist/helpers.d.ts",
            "import": "./dist/helpers.js",
            "require": "./dist/helpers.cjs"
        },
        "./parameters": {
            "types": "./dist/parameters.d.ts",
            "import": "./dist/parameters.js",
            "require": "./dist/parameters.cjs"
        },
        "./responses": {
            "types": "./dist/responses.d.ts",
            "import": "./dist/responses.js",
            "require": "./dist/responses.cjs"
        },
        "./routing": {
            "types": "./dist/routing.d.ts",
            "import": "./dist/routing.js",
            "require": "./dist/routing.cjs"
        },
        "./cors": {
            "types": "./dist/middleware/cors.d.ts",
            "import": "./dist/middleware/cors.js",
            "require": "./dist/middleware/cors.cjs"
        },
        "./trusted-host": {
            "types": "./dist/middleware/trusted-host.d.ts",
            "import": "./dist/middleware/trusted-host.js",
            "require": "./dist/middleware/trusted-host.cjs"
        },
        "./compression": {
            "types": "./dist/middleware/compression.d.ts",
            "import": "./dist/middleware/compression.js",
            "require": "./dist/middleware/compression.cjs"
        },
        "./aws-lambda": {
            "types": "./dist/adapters/aws-lambda.d.ts",
            "import": "./dist/adapters/aws-lambda.js",
            "require": "./dist/adapters/aws-lambda.cjs"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "rimraf dist && rollup -c && bun pm pack --dry-run",
        "package": "bun run build && bun pm pack",
        "prettify": "prettier --check src tests README.md || (prettier -w src tests README.md)",
        "prepare": "husky install",
        "test": "jest --no-cache --runInBand --config jestconfig.json --verbose",
        "test:cov": "jest --no-cache --runInBand --config jestconfig.json --verbose --coverage",
        "docs:ref": "typedoc --options typedoc.config.js",
        "docs:dev": "vitepress dev docs",
        "docs:build": "vitepress build docs",
        "docs:serve": "vitepress serve docs"
    },
    "publishConfig": {
        "access": "public"
    },
    "keywords": [
        "cerces",
        "web",
        "app",
        "http",
        "application",
        "framework",
        "router",
        "serverless",
        "cloudflare",
        "workers",
        "compute",
        "bun",
        "aws",
        "lambda",
        "functions",
        "openapi",
        "swagger",
        "openapi",
        "oas3.1",
        "typescript",
        "nodejs",
        "typed"
    ],
    "author": "Ian Huang (https://github.com/ianhco)",
    "license": "MIT",
    "homepage": "https://github.com/ianhco/cerces",
    "repository": {
        "type": "git",
        "url": "git@github.com:ianhco/cerces.git"
    },
    "bugs": {
        "url": "https://github.com/ianhco/cerces/issues"
    },
    "devDependencies": {
        "@commitlint/cli": "^13.1.0",
        "@commitlint/config-conventional": "^13.1.0",
        "@rollup/plugin-terser": "^0.2.1",
        "@rollup/plugin-typescript": "^10.0.1",
        "@types/aws-lambda": "^8.10.152",
        "@types/jest": "^29.0.0",
        "commitlint": "^20.1.0",
        "drizzle-kit": "^0.30.4",
        "drizzle-orm": "^0.39.3",
        "drizzle-zod": "^0.5.1",
        "husky": "^7.0.2",
        "jest": "^29.0.0",
        "prettier": "^3.1.0",
        "rimraf": "^5.0.5",
        "rollup": "^3.25.1",
        "ts-jest": "^29.1.0",
        "typedoc": "^0.28.2",
        "typedoc-plugin-markdown": "^4.6.2",
        "typescript": "^5.1.3",
        "vitepress": "^2.0.0-alpha.12",
        "vue": "^3.4.21"
    },
    "peerDependencies": {
        "zod": "^4.1.0",
        "@asteasolutions/zod-to-openapi": "^8.1.0",
        "typescript": "^5"
    }
}
